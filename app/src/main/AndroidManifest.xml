<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group4.wonderwall" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="group4.wonderwall.SongActivity"
            android:label="@string/title_activity_song" >
        </activity>
        <activity
            android:name="group4.wonderwall.SongChooserActivity"
            android:label="@string/title_activity_song_chooser" >
        </activity>
        <activity
            android:name=".SongCompleted"
            android:label="@string/title_activity_song_completed" >
        </activity>
    </application>

</manifest>
